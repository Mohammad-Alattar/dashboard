datasource db {
  provider = "sqlite"
  url      = "file:./db/database.db"
}

// generator
generator client {
  provider = "prisma-client-py"
}

model Guild {
  id     BigInt   @id @unique
  prefix Prefix[]
}

model Prefix {
  count      Int  @id @default(autoincrement())
  prefix String
  Guild   Guild  @relation(fields: [guildId], references: [id])
  guildId BigInt
}
